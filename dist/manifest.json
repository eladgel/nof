{
  "manifest_version": 3,
  "name": "Chrome Extension Starter",
  "version": "1.0.0",
  "description": "Get started with building your dream Chrome extension effortlessly! Our starter kit combines the power of cutting-edge technologies like Vite, TypeScript, React, CRX, Tailwind CSS, and DaisyUI, making extension development a breeze.",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "options_page": "src/options/index.html",
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/index.prod.tsx-loader-BaCOcuw0.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "*.js",
        "*.css",
        "public/*",
        "assets/fonts/Inter-Black.ttf",
        "assets/fonts/Inter-ExtraBold.ttf",
        "assets/fonts/Inter-Bold.ttf",
        "assets/fonts/Inter-SemiBold.ttf",
        "assets/fonts/Inter-Medium.ttf",
        "assets/fonts/Inter-Regular.ttf",
        "assets/fonts/Inter-Light.ttf",
        "assets/fonts/Inter-ExtraLight.ttf",
        "assets/fonts/Inter-Thin.ttf",
        "assets/createShadowRoot-7Xa9_V-K.js",
        "assets/index.prod.tsx-Cp9iQm13.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
